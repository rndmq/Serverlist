task.spawn(function()
while true do
if OpUpg and not hasRun then
hasRun = true
op()
break
end
task.wait(0.5)
end
end)

task.spawn(function()
while true do
if ThOpUpg and not ThhasRun then
RhhasRun = true
Thop()
break
end
task.wait(0.5)
end
end)