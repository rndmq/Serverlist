MainS:CreateToggle("Add Grass", function(state)
Grass = state
if state then
task.spawn(function()
while Grass do
local args = {
	{
		normal = 0,
		ruby = 0,
		silver = 0,
		golden = 0,
		diamond = 999
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GrassCollect"):FireServer(unpack(args))
task.wait(0.1) 
end 
end)
end
end)

MainS:CreateToggle("Auto upg Tier", function(state)
tier = state
if state then
task.spawn(function()
while tier do
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("TierUpgrade"):FireServer()
task.wait(1)
end 
end)
end
end)

MainS:CreateToggle("Upgrade Tree", function(state)
Tree = state
if state then
task.spawn(function()
while Tree do
for i = 1, 20 do
local args = {
	{
		treeType = 1,
		treeNumber = i
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("UpgradeTree"):FireServer(unpack(args))
task.wait(0.2) 
end
task.wait(0.1)
end
end)
end
end)

