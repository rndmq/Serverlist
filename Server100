local function Thop()
SSupg:CreateToggle("Auto Upgrade All Stat", function(state)
    AutoAllStat = state
    if state then
        task.spawn(function()
            local Remote = game:GetService("ReplicatedStorage")
                :WaitForChild("Remotes")
                :WaitForChild("Upgrade")

            local statList = {
                "Stat1",
                "Stat2",
                "Stat3",
                "Stat4",
                "Stat5",
                "BronzeValue"
            }

            while AutoAllStat do
                for _, stat in ipairs(statList) do
                    if not AutoAllStat then break end
                    local amount = 999
                    local max = 999
                    if stat == "Stat1" then
                        amount = 50
                        max = 50
                    end

                    Remote:FireServer({
                        {
                            currencyName = "StatPoint",
                            autoBuy = false,
                            amount = amount,
                            max = max,
                            upgradeValue = stat,
                            cost = 0
                        }
                    })

                    task.wait(0.2)
                end

                task.wait(1)
            end
        end)
    end
end)
end


