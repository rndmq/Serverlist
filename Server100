local function op()
local O = rndm:CreateTab("Op Upgrade")
local Op = O:CreateSection("Grass")
local Opp = O:CreateSection("Pop")
Op:CreateToggle("OP ALL UPGRADE (QUEUE)", function(state)
    AllUpgrade = state
    if state then
        task.spawn(function()
            while AllUpgrade do
                local Remote = game:GetService("ReplicatedStorage")
                    :WaitForChild("Remotes")
                    :WaitForChild("Upgrade")

                local upgradeQueue = {
                    -- Grass
                    {currencyName="Grass", amount=999, max=999, upgradeValue="GrassAmount"},
                    {currencyName="Grass", amount=100, max=100, upgradeValue="GrassValue"},
                    {currencyName="Grass", amount=999, max=999, upgradeValue="SpawnRate1"},

                    -- Rebirth
                    {currencyName="Rebirth", amount=100, max=100, upgradeValue="GrassValue2"},
                    {currencyName="Rebirth", amount=999, max=999, upgradeValue="BladeRadius"},
                    {currencyName="Rebirth", amount=999, max=999, upgradeValue="SpawnRate2"},

                    -- Pop
                    {currencyName="Pop", amount=100, max=100, upgradeValue="GrassValue4"},
                    {currencyName="Pop", amount=999, max=999, upgradeValue="RebirthValue2"},
                    {currencyName="Pop", amount=999, max=999, upgradeValue="AutoClick"},
                }

                for _, upg in ipairs(upgradeQueue) do
                    if not AllUpgrade then break end

                    Remote:FireServer({
                        {
                            currencyName = upg.currencyName,
                            autoBuy = false,
                            amount = upg.amount,
                            max = upg.max,
                            upgradeValue = upg.upgradeValue,
                            cost = 1
                        }
                    })

                    task.wait(0.2)
                end

                task.wait(1)
            end
        end)
    end
end)
Op:CreateToggle("Op Upgrade Grass amount", function(state)
OpupgGM = state
-- PLEASE READ: ANYTHING ABOVE 999 WILL RESULTING A NAN VALUE.
if state then
task.spawn(function()
while OpupgGM do
local args = {
	{
		{
			currencyName = "Grass",
			autoBuy = false,
			amount = 999,
			max = 999,
			upgradeValue = "GrassAmount",
			cost = 1
		}
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Upgrade"):FireServer(unpack(args))
task.wait(1)
end
end)
end
end)

